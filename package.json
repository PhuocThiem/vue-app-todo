{
  "name": "vue-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve-staging": "vue-cli-service serve --mode staging",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:js": "eslint . --ext .js,.vue",
    "lint:js:fix": "yarn run lint:js -- --fix"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.3.2",
    "lodash": "^4.17.15",
    "serialize-error": "^5.0.0",
    "vee-validate": "^3.0.11",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-airbnb": "^4.0.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "no-console": 0,
      "no-debugger": 0,
      "import/prefer-default-export": 0,
      "import/no-cycle": 0,
      "no-shadow": 1,
      "comma-dangle": 0,
      "import/extensions": 0,
      "import/no-unresolved": [2, {
        "amd": true,
        "ignore": ["@/"]
      }],
      "space-before-function-paren": 0,
      "arrow-parens": [2, "as-needed"]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "yarn run lint:js:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
